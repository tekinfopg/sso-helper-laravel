{
  "name": "tekinfopg/sso-helper-laravel",
  "description": "Integrate Laravel with Keycloak",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "keycloak",
    "sso",
    "laravel"
  ],
  "version": "1.1.2",
  "require": {
    "php": "^7.4 || ^8.0 || ^8.1",
    "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
    "socialiteproviders/keycloak": "^5.3.0",
    "socialiteproviders/manager": "^4.1",
    "laravel/socialite": "^5.18",
    "guzzlehttp/guzzle": "^7.8.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Edoaurahman\\KeycloakSso\\KeycloakServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Edoaurahman\\KeycloakSso\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Ridho Aulia' Rahman",
